{
  "name": "php/programming-challenge",
  "description": "PHP Programming Challenge",
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "^9.4.4",
    "phpstan/phpstan": "^0.12.92",
    "roave/security-advisories": "dev-latest",
    "symplify/easy-coding-standard": "^9.4.23",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "ProgrammingChallenge\\": "tests/ProgrammingChallenge",
      "ProgrammingChallenge\\ArmstrongNumbers\\": "src/ArmstrongNumbers",
      "ProgrammingChallenge\\Brackets\\": "src/Brackets",
      "ProgrammingChallenge\\Distincts\\": "src/Distincts"
    }
  },
  "scripts": {
    "ArmstrongNumbers-PhpStan": "vendor/bin/phpstan analyse src/ArmstrongNumbers tests/ProgrammingChallenge/ArmstrongNumbers --level=7 --ansi",
    "ArmstrongNumbers-Ecs": "vendor/bin/ecs check src/ArmstrongNumbers tests/ProgrammingChallenge/ArmstrongNumbers --clear-cache",
    "ArmstrongNumbers-Test": "vendor/bin/phpunit --testsuite ArmstrongNumbers",
    "Brackets-PhpStan": "vendor/bin/phpstan analyse src/Brackets tests/ProgrammingChallenge/Brackets --level=7 --ansi",
    "Brackets-Ecs": "vendor/bin/ecs check src/Brackets tests/ProgrammingChallenge/Brackets --clear-cache",
    "Brackets-Test": "vendor/bin/phpunit --testsuite Brackets",
    "Distincts-PhpStan": "vendor/bin/phpstan analyse src/Distincts tests/ProgrammingChallenge/Distincts --level=7 --ansi",
    "Distincts-Ecs": "vendor/bin/ecs check src/Distincts tests/ProgrammingChallenge/Distincts --clear-cache",
    "Distincts-Test": "vendor/bin/phpunit --testsuite Distincts"
  }
}